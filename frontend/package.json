{
  "name": "studio1way",
  "version": "0.1.0",
  "description": "studio 1way website",
  "main": "/app/frontend/src/index.tsx",
  "scripts": {
    "build": "webpack-cli --mode production --config=/app/frontend/config/webpack/prod.js",
    "clean-static": "rm -rfd /app/src/main/resources/static/*",
    "lint": "npx eslint /app/frontend/src && tsc --skipLibCheck && prettier --check /app/frontend/src/assets/scss",
    "lint-fix": "npx eslint /app/frontend/src --fix && prettier --write /app/frontend/src/assets/scss",
    "start-dev": "webpack serve --config=/app/frontend/config/webpack/dev.js",
    "test": "jest --config=/app/frontend/jest.config.js",
    "test-ci": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fonsecapeter/studio1way.git"
  },
